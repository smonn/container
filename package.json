{
  "name": "@smonn/container",
  "version": "1.2.1",
  "description": "TypeScript-based dependency injection container",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Simon Ingeson",
    "url": "https://smonn.se"
  },
  "keywords": [
    "async",
    "browser",
    "browsers",
    "node",
    "nodejs",
    "container",
    "inversion of control",
    "ioc",
    "dependency injection",
    "di",
    "dependency",
    "injection",
    "typescript",
    "universal"
  ],
  "main": "./dist/container.cjs",
  "module": "./dist/container.mjs",
  "exports": {
    ".": {
      "require": "./dist/container.cjs",
      "import": "./dist/container.mjs"
    }
  },
  "typings": "dist/container.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=16",
    "npm": ">=6"
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "format:scripts": "prettier . --write",
    "format": "npm run format:scripts && npm run format:styles",
    "prepare": "husky install && husky add .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/jest": "^28.1.6",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.6.4",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "dts-bundle-generator": "^6.12.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "repository": "github:smonn/container"
}
