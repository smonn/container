{
  "name": "@smonn/container",
  "version": "1.2.1",
  "description": "Easy to use dependency injection container",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "author": {
    "name": "Simon Ingeson",
    "url": "https://smonn.se"
  },
  "keywords": [
    "async",
    "browser",
    "browsers",
    "node",
    "nodejs",
    "container",
    "inversion of control",
    "ioc",
    "dependency injection",
    "di",
    "dependency",
    "injection",
    "typescript",
    "universal"
  ],
  "main": "dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "typings": "dist/types/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "scripts": {
    "clean": "rimraf dist",
    "check": "tsc --noEmit",
    "build": "rollup -c && ./scripts/fixup.sh",
    "lint": "eslint --ext ts .",
    "format": "prettier --write .",
    "test": "uvu -r ts-node/register test",
    "coverage": "c8 --include=src npm test",
    "report": "c8 report --reporter=text-lcov > coverage/lcov.info",
    "report:html": "c8 report --reporter=html && open ./coverage/index.html",
    "prepublishOnly": "run-s clean lint check test build",
    "prepare": "husky install",
    "sample": "ts-node src/readme-sample.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "c8": "7.12.0",
    "esbuild": "0.14.39",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-unicorn": "42.0.0",
    "husky": "8.0.1",
    "lint-staged": "12.4.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "rollup": "2.74.1",
    "rollup-plugin-dts": "4.2.1",
    "rollup-plugin-esbuild": "4.9.1",
    "rollup-plugin-filesize": "9.1.2",
    "ts-node": "10.8.0",
    "typescript": "4.7.2",
    "uvu": "0.5.3"
  },
  "repository": "github:smonn/container",
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
